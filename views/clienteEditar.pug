extends layout.pug
include mixins/formCliente.pug
block contenido

    .container
        
        if error
            .alert.alert-danger(role='alert') 
                each err in error
                    .alert=err.message
            
        
        if errorControl
            .alert.alert-danger(role='alert') 
                each err in errorControl
                    .alert=err
       
        +formCliente(resCliente)